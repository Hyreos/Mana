    str? p1

    @serialize(0x15 + 6 / 8 + 8 * 9) 
    str? p2
    i64 p3
    i64 p4
    f64 p5 = 0.45555555
    f32 p6 = 0.42222f
}

component B : A {
    @serialize(1) 
    str p1
    f64 random_float_field = 55655.45554

    @serialize(2)
    str p2
}